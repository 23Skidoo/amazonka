instance ToQuery {{ scope.name }} where
{% if scope.http.query | mapLength > 0%}
    toQuery {{ scope.name }}{..} =
  {% for part in scope.http.query %}
    {% if part.first %}
        [ "{{ part.key }}" =? {{ part.value }}
    {% else %}
        , "{{ part.key }}" =? {{ part.value }}
    {% endif %}
    {% if part.last %}
        ]
    {% endif %}
  {% endfor %}
{% else %}
    toQuery = const mempty
{% endif %}
