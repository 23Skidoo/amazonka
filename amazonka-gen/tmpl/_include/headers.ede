{% if scope.headers | listLength > 0 %}
instance ToHeaders {{ scope.name }} where
    toHeaders {{ scope.name }}{..} = concat
  {% for hdr in scope.headers %}
    {% if hdr.first %}
        [ "{{ hdr.value.location_name }}" =: {{ hdr.value.prefixed }}
    {% else %}
        , "{{ hdr.value.location_name }}" =: {{ hdr.value.prefixed }}
    {% endif %}
  {% endfor %}
        ]
{% else %}
instance ToHeaders {{ scope.name }}
{% endif %}
