        , Network.AWS.EMR.V2009_03_31.AddTags
        , Network.AWS.EMR.V2009_03_31.ListInstances
        , Network.AWS.EMR.V2009_03_31.ListClusters

    build-depends:
          amazonka-core
        , base          == 4.7.*
