<?xml version="1.0" encoding="UTF-8"?>
<CreateHostedZoneResponse xmlns="https://route53.amazonaws.com/doc/2012-12-12/">
  {{#chzrHostedZone}}
  <HostedZone>
    <Id>{{hzId}}</Id>
    <Name>{{hzName}}</Name>
    <CallerReference>{{hzCallerReference}}</CallerReference>
    {{#hzConfig}}
    <Config>
      <Comment>{{cComment}}</Comment>
    </Config>
    {{/hzConfig}}
    <ResourceRecordSetCount>{{hzResourceRecordSetCount}}</ResourceRecordSetCount>
  </HostedZone>
  {{/chzrHostedZone}}
  {{#chzrChangeInfo}}
  <ChangeInfo>
    <Id>{{ciId}}</Id>
    <Status>{{ciStatus}}</Status>
    <SubmittedAt>{{ciSubmittedAt}}</SubmittedAt>
  </ChangeInfo>
  {{/chzrChangeInfo}}
  {{#chzrDelegationSet}}
  <DelegationSet>
    <NameServers>
    {{#dsNameServers}}
      <NameServer>{{.}}</NameServer>
    {{/dsNameServers}}
    </NameServers>
  </DelegationSet>
  {{/chzrDelegationSet}}
</CreateHostedZoneResponse>
