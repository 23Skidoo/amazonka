<?xml version="1.0" encoding="UTF-8"?>
<GetHealthCheckResponse xmlns="https://route53.amazonaws.com/doc/2012-12-12/">
  {{#ghcrHealthCheck}}
  <HealthCheck>
    <Id>{{hcId}}</Id>
    <CallerReference>{{hcCallerReference}}</CallerReference>
    {{#hcHealthCheckConfig}}
    <HealthCheckConfig>
      <IPAddress>{{hccIPAddress}}</IPAddress>
      <Port>{{hccPort}}</Port>
      <Type>{{hccType}}</Type>
      <ResourcePath>{{hccResourcePath}}</ResourcePath>
      <FullyQualifiedDomainName>{{hccFullyQualifiedDomainName}}</FullyQualifiedDomainName>
    </HealthCheckConfig>
    {{/hcHealthCheckConfig}}
  </HealthCheck>
  {{/ghcrHealthCheck}}
</GetHealthCheckResponse>
