{% case field.location %}
{% when "headers" %}
        {% include "applicative.ede" %} h ~:: "{{ field.locationName }}"
{% when "header" %}
        {% include "applicative.ede" %} h {% if field.required %}~:{% else %}~:?{% endif %} "{{ field.value.locationName }}"
{% when "status-code" %}
        {% include "applicative.ede" %} o .: "{{ field.locationName }}"
{% when "body" %}
        {% include "applicative.ede" %} pure (RsBody b)
{% endcase %}
