
instance FromXML {{ type.name }} where
{% case type.type %}
{% when "nullary" %}
    parseXML = fromXMLText "{{ type.name }}"
{% else %}
    parseXML c = {{ type.name }}
  {% for field in type.fields %}
        {% include "applicative.ede" %} c {% include "operator.ede" %} "{{ field.value.locationName }}"
  {% endfor %}
{% endcase %}
