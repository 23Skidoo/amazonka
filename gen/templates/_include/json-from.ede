
instance FromJSON {{ type.name }} where
{% case type.type %}
{% when "nullary" %}
    parseJSON = fromJSONText "{{ type.name }}"
{% else %}
    parseJSON = withObject "{{ type.name }}" $ \o -> {{ type.name }}
  {% for field in type.fields %}
        {% include "applicative.ede" %} o .: "{{ field.value.locationName }}"
  {% endfor %}
{% endcase %}
