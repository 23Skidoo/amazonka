module {{ moduleName }}
    (
    -- * Request
      {{ input.name }}
    -- ** Request constructor
    , {{ input.constructor.name}}
  {% for lens in input.lenses %}
    {% if lens.first %}
    -- ** Request lenses
    {% endif %}
