name:                  amazonka-s3-examples
version:               0.1.2
synopsis:              Amazon Simple Storage Service SDK Examples
homepage:              https://github.com/brendanhay/amazonka
license:               MPL-2.0
author:                Brendan Hay
maintainer:            Brendan Hay <brendan.g.hay@gmail.com>
copyright:             Copyright (c) 2013-2014 Brendan Hay
category:              Network, AWS, Cloud, Distributed Computing
build-type:            Simple
cabal-version:         >= 1.10

library
    default-language:  Haskell2010
    hs-source-dirs:    src

    ghc-options:       -Wall -threaded

    exposed-modules:
        S3

    build-depends:
        amazonka      == 0.1.2.*
      , amazonka-core == 0.1.2.*
      , amazonka-s3 == 0.1.2.*
      , base          >= 4.7     && < 5
      , bytestring    >= 0.9
      , conduit       >= 1.1     && < 1.3
      , lens          >= 4.3     && < 5
      , lifted-base
      , text          >= 1.1
      , time          >= 1.4
      , transformers  == 0.4.*
