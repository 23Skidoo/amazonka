{-# LANGUAGE DataKinds                  #-}
{-# LANGUAGE DeriveGeneric              #-}
{-# LANGUAGE FlexibleInstances          #-}
{-# LANGUAGE GeneralizedNewtypeDeriving #-}
{-# LANGUAGE LambdaCase                 #-}
{-# LANGUAGE NoImplicitPrelude          #-}
{-# LANGUAGE OverloadedStrings          #-}
{-# LANGUAGE RecordWildCards            #-}
{-# LANGUAGE TypeFamilies               #-}
{-# LANGUAGE ViewPatterns               #-}

{% include "_include/license.ede" %}
--
-- Derived from AWS service descriptions, licensed under Apache 2.0.

module {{ moduleName }}
    (
    -- * Service
      {{ serviceAbbrev }}
    -- ** Errors
    , {{ serviceError }}

  {% for shape in shapes %}
    -- * {{ shape.value.name }}
    {% case shape.value.type %}
    {% when "product" %}
    , {{ shape.value.name }}
      {% for lens in shape.value.lenses %}
    , {{ lens.value.name }}
      {% endfor %}
    {% when "sum" %}
    , {{ shape.value.name }} (..)
    {% endcase %}
    {% if !shape.last %}

    {% endif %}
  {% endfor %}
    ) where

import qualified GHC.Exts
import           Network.AWS.Prelude
import           Network.AWS.Signing
{% for import in typeImports %}
import           {{ import.value }}
{% endfor %}

{% include "_include/service.ede" %}
{% for shape in shapes %}
  {% case shape.value.type %}
  {% when "product" %}

    {% include "_include/product.ede" with shape = shape.value %}
  {% when "sum" %}

    {% include "_include/sum.ede" with shape = shape.value %}
  {% endcase %}
{% endfor %}
